{
  "name": "angular-nx-playground",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "start": "nx serve",
    "start:msw": "nx serve -c=msw",
    "build": "nx build",
    "test": "nx test --runInBand",
    "e2e:msw": "nx e2e movie-db-e2e --watch --browser=chrome",
    "lint:fix:all": "nx run-many --all --target=lint --fix --parallel=3 && pnpm stylelint:fix",
    "stylelint": "stylelint {apps,libs}/**/**/*.scss",
    "stylelint:fix": "pnpm stylelint --fix",
    "postinstall": "node ./decorate-angular-cli.js",
    "prepare": "is-ci || husky install",
    "nx:migrate": "pnpm dlx nx migrate latest",
    "nx:run-migration": "pnpm dlx nx migrate --run-migrations --create-commits --commit-prefix='chore(core): AUTOMATED - '"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "16.1.3",
    "@angular/common": "16.1.3",
    "@angular/compiler": "16.1.3",
    "@angular/core": "16.1.3",
    "@angular/forms": "16.1.3",
    "@angular/platform-browser": "16.1.3",
    "@angular/platform-browser-dynamic": "16.1.3",
    "@angular/router": "16.1.3",
    "@babel/runtime": "~7.22.5",
    "@nx/angular": "16.4.3",
    "modern-normalize": "~2.0.0",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.0",
    "zod": "~3.21.4",
    "zone.js": "0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "16.1.3",
    "@angular-devkit/core": "16.1.3",
    "@angular-devkit/schematics": "16.1.3",
    "@angular-eslint/eslint-plugin": "16.0.3",
    "@angular-eslint/eslint-plugin-template": "16.0.3",
    "@angular-eslint/template-parser": "16.0.3",
    "@angular/cli": "~16.1.3",
    "@angular/compiler-cli": "16.1.3",
    "@commitlint/cli": "~17.6.6",
    "@commitlint/config-conventional": "~17.6.6",
    "@commitlint/types": "~17.4.4",
    "@cypress/webpack-dev-server": "^3.5.0",
    "@nx/cypress": "16.4.3",
    "@nx/eslint-plugin": "16.4.3",
    "@nx/jest": "16.4.3",
    "@nx/js": "16.4.3",
    "@nx/linter": "16.4.3",
    "@nx/workspace": "16.4.3",
    "@schematics/angular": "16.1.3",
    "@swc-node/register": "~1.6.6",
    "@swc/core": "1.3.66",
    "@testing-library/angular": "~14.1.1",
    "@testing-library/cypress": "~9.0.0",
    "@testing-library/jest-dom": "~5.16.5",
    "@testing-library/user-event": "~14.4.3",
    "@types/jest": "29.5.2",
    "@types/node": "18.15.5",
    "@types/testing-library__cypress": "5.0.9",
    "@types/testing-library__jest-dom": "~5.14.6",
    "@typescript-eslint/eslint-plugin": "5.60.1",
    "@typescript-eslint/parser": "5.60.1",
    "cypress": "^12.16.0",
    "dotenv": "~16.3.1",
    "eslint": "~8.43.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-cypress": "2.13.3",
    "eslint-plugin-jest": "27.2.2",
    "eslint-plugin-jest-dom": "5.0.1",
    "eslint-plugin-rxjs": "~5.0.3",
    "eslint-plugin-rxjs-angular": "~2.0.1",
    "eslint-plugin-testing-library": "~5.11.0",
    "eslint-plugin-ui-testing": "~2.0.1",
    "eslint-plugin-unicorn": "~47.0.0",
    "html-webpack-plugin": "^5.5.3",
    "husky": "~8.0.3",
    "is-ci": "~3.0.1",
    "jest": "29.5.0",
    "jest-environment-jsdom": "29.5.0",
    "jest-preset-angular": "13.1.1",
    "msw": "0.0.0-fetch.rc-14",
    "ng-packagr": "16.1.0",
    "nx": "16.4.3",
    "nx-cloud": "16.0.5",
    "postcss": "~8.4.24",
    "postcss-import": "~15.1.0",
    "postcss-preset-env": "~8.5.1",
    "postcss-url": "~10.1.3",
    "prettier": "~2.8.8",
    "stylelint": "15.9.0",
    "stylelint-8-point-grid": "2.2.0",
    "stylelint-color-format": "1.1.0",
    "stylelint-config-standard-scss": "9.0.0",
    "ts-jest": "29.1.0",
    "ts-node": "10.9.1",
    "typescript": "5.1.3",
    "webpack": "^5.88.0"
  },
  "msw": {
    "workerDirectory": "libs/shared/test/msw/src/assets"
  },
  "lint-staged": {
    "{apps,libs}/**/**/*.{css,scss}": [
      "pnpm stylelint:fix"
    ],
    "{apps,libs}/**/**/*.{html,css,scss,md,js,jsx,ts,tsx,graphql,yml}": [
      "nx affected:lint --fix=true --base=main",
      "nx format:write --uncommitted"
    ]
  }
}
