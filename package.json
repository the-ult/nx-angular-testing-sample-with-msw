{
  "name": "angular-nx-playground",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "start": "nx serve",
    "start:msw": "nx serve -c=msw",
    "build": "nx build",
    "test": "nx test --runInBand",
    "e2e:msw": "nx e2e movie-db-e2e --watch --browser=chrome",
    "lint:all:fix": "nx run-many --all --target=lint --fix --parallel=3 && pnpm stylelint:fix",
    "stylelint": "stylelint {apps,libs}/**/**/*.scss",
    "stylelint:fix": "pnpm stylelint --fix",
    "postinstall": "node ./decorate-angular-cli.js",
    "prepare": "is-ci || husky install",
    "nx:migrate": "pnpm dlx nx migrate latest",
    "nx:run-migration": "pnpm dlx nx migrate --run-migrations"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "17.1.0",
    "@angular/common": "17.1.0",
    "@angular/compiler": "17.1.0",
    "@angular/core": "17.1.0",
    "@angular/forms": "17.1.0",
    "@angular/platform-browser": "17.1.0",
    "@angular/platform-browser-dynamic": "17.1.0",
    "@angular/router": "17.1.0",
    "@babel/runtime": "~7.23.2",
    "@nx/angular": "17.2.8",
    "modern-normalize": "~2.0.0",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.2",
    "zod": "~3.22.4",
    "zone.js": "0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "17.1.0",
    "@angular-devkit/core": "17.1.0",
    "@angular-devkit/schematics": "17.1.0",
