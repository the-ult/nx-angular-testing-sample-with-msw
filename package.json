{
  "name": "angular-nx-playground",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "start": "nx serve",
    "start:msw": "nx serve -c=msw",
    "build": "nx build",
    "test": "nx test --runInBand",
    "e2e:msw": "nx e2e movie-db-e2e --watch --browser=chrome",
    "lint:fix:all": "nx run-many --all --target=lint --fix --parallel=3 && pnpm stylelint:fix",
    "stylelint": "stylelint {apps,libs}/**/**/*.scss",
    "stylelint:fix": "pnpm stylelint --fix",
    "postinstall": "node ./decorate-angular-cli.js",
    "prepare": "is-ci || husky install",
    "nx:migrate": "pnpm dlx nx migrate latest",
    "nx:run-migration": "pnpm dlx nx migrate --run-migrations --create-commits --commit-prefix='chore(core): AUTOMATED - '"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "16.2.5",
    "@angular/common": "16.2.5",
    "@angular/compiler": "16.2.5",
    "@angular/core": "16.2.5",
    "@angular/forms": "16.2.5",
    "@angular/platform-browser": "16.2.5",
    "@angular/platform-browser-dynamic": "16.2.5",
    "@angular/router": "16.2.5",
    "@babel/runtime": "~7.22.15",
    "@nx/angular": "16.8.1",
    "modern-normalize": "~2.0.0",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.2",
    "zod": "~3.22.2",
    "zone.js": "0.13.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "16.2.2",
    "@angular-devkit/core": "16.2.2",
    "@angular-devkit/schematics": "16.2.2",
    "@angular-eslint/eslint-plugin": "16.1.2",
    "@angular-eslint/eslint-plugin-template": "16.1.2",
    "@angular-eslint/template-parser": "16.1.2",
    "@angular/cli": "~16.2.2",
    "@angular/compiler-cli": "16.2.5",
    "@commitlint/cli": "~17.7.1",
    "@commitlint/config-conventional": "~17.7.0",
    "@commitlint/types": "~17.4.4",
    "@cypress/webpack-dev-server": "^3.6.1",
    "@nx/cypress": "16.8.1",
    "@nx/eslint-plugin": "16.8.1",
    "@nx/jest": "16.8.1",
    "@nx/js": "16.8.1",
    "@nx/linter": "16.8.1",
    "@nx/workspace": "16.8.1",
    "@schematics/angular": "16.2.2",
    "@swc-node/register": "~1.6.7",
    "@swc/core": "1.3.84",
    "@testing-library/angular": "~14.3.0",
    "@testing-library/cypress": "~10.0.0",
    "@testing-library/jest-dom": "~6.1.3",
    "@testing-library/user-event": "~14.4.3",
    "@types/jest": "29.5.4",
    "@types/node": "18.17.14",
    "@types/testing-library__cypress": "5.0.10",
    "@types/testing-library__jest-dom": "~6.0.0",
    "@typescript-eslint/eslint-plugin": "6.7.0",
    "@typescript-eslint/parser": "6.7.0",
    "cypress": "^13.2.0",
    "dotenv": "~16.3.1",
    "eslint": "8.49.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-cypress": "2.14.0",
    "eslint-plugin-jest": "27.2.3",
    "eslint-plugin-jest-dom": "5.1.0",
    "eslint-plugin-rxjs": "~5.0.3",
    "eslint-plugin-rxjs-angular": "~2.0.1",
    "eslint-plugin-testing-library": "~6.0.1",
    "eslint-plugin-ui-testing": "~2.0.1",
    "eslint-plugin-unicorn": "~48.0.1",
    "html-webpack-plugin": "^5.5.3",
    "husky": "~8.0.3",
    "is-ci": "~3.0.1",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-preset-angular": "13.1.1",
    "msw": "0.0.0-fetch.rc-19",
    "ng-packagr": "16.2.3",
    "nx": "16.8.1",
    "nx-cloud": "16.4.0",
    "postcss": "~8.4.29",
    "postcss-import": "~15.1.0",
    "postcss-preset-env": "~9.1.3",
    "postcss-url": "~10.1.3",
    "prettier": "~3.0.3",
    "prettier-plugin-organize-imports": "3.2.3",
    "stylelint": "15.10.3",
    "stylelint-8-point-grid": "2.2.0",
    "stylelint-color-format": "1.1.0",
    "stylelint-config-standard-scss": "11.0.0",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "ts-reset": "0.0.1",
    "typescript": "5.1.6",
    "webpack": "^5.88.2"
  },
  "msw": {
    "workerDirectory": "libs/shared/test/msw/src/assets"
  },
  "lint-staged": {
    "{apps,libs}/**/**/*.{css,scss}": [
      "pnpm stylelint:fix"
    ],
    "{apps,libs}/**/**/*.{html,css,scss,md,js,jsx,ts,tsx,graphql,yml}": [
      "nx affected:lint --fix=true --base=main",
      "nx format:write --uncommitted"
    ]
  }
}
